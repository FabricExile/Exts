namespace Tool {

  /// Screen space rectangle drawn with lines.
  ///
  /// See :ref:`TPG.drawing` for more information about drawing tool shapes.
  object RectangleLinesShape : GeometricShape{
  };

  RectangleLinesShape() {
    this.setGeometry( WGetXYRectangleLines() );
    this.setTransformSpace( NormalizedViewportSpace );
  }

}// namespace Tool
