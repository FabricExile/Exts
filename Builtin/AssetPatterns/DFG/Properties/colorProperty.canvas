  "code" : "dfgEntry {
  String k = key;
  if(k == 'custom')
    k = custom;
  if(!this.hasProperty(k))
    return;
  Ref<ImporterObject> mutableThis = this;
  mutableThis.updateProperty(k, this.defaultContext);
  result = mutableThis.getColorProperty(k);
}
