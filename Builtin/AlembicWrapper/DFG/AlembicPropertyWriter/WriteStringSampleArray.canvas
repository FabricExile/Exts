  "code" : "require AlembicWrapper;

operator AlembicWrapper_AlembicPropertyWriter_writeStringSample_task<<<index>>>(
  io Boolean[] result
, io AlembicPropertyWriter[] this
, in String[] value
) {
  result[index] = this[index < this.size() ? index : this.size() - 1].writeStringSample(value[index < value.size() ? index : value.size() - 1]);
}

dfgEntry {
  if( this.size() == 0) return;
  if( value.size() == 0) return;
  Size task_size = this.size();
  if(task_size < value.size()) task_size = value.size();
  result.resize(task_size);
  for(Size index = 0; index < task_size; index++) {
    AlembicWrapper_AlembicPropertyWriter_writeStringSample_task(index, result, this, value);
  }
}
