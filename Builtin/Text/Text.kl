/**
  \versionadded 2.4.0
  
  Text interface defines methods to draw text that depend of the OpenGL/DirectX(not supported) version/implemention.
  They should be implemented according to the renderer used.
  
  \seealso :kl-ref:`BaseTextGL`.

  \dfgPresetOmit
*/
interface Text {
  /// Prepares rendering.
  prepareRender!();

  /// Finishs rendering.
  finishRender!();
};
