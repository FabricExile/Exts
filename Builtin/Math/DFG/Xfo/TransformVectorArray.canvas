  "code" : "require Math;

operator Math_Xfo_transformVector_task<<<index>>>(
  io Vec3[] result
, in Xfo[] this
, in Vec3[] v
) {
  result[index] = this[index < this.size() ? index : this.size() - 1].transformVector(v[index < v.size() ? index : v.size() - 1]);
}

dfgEntry {
  if( this.size() == 0) return;
  if( v.size() == 0) return;
  Size task_size = this.size();
  if(task_size < v.size()) task_size = v.size();
  result.resize(task_size);
  if(task_size < 10000 /* pex margin */) {
    for(Size index = 0; index < task_size; index++) {
      Math_Xfo_transformVector_task(index, result, this, v);
    }
  } else {
    Math_Xfo_transformVector_task<<<task_size>>>(result, this, v);
  }
}
